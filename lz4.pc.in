prefix=@prefix@
exec_prefix=@exec_prefix@
libdir=@libdir@
includedir=@includedir@

Name: @PACKAGE_NAME@
Description: LZ4 C library.
Version: @PACKAGE_VERSION@
URL: @PACKAGE_URL@
Libs: -L${libdir} -llz4-@LZ4_API_VERSION@
Cflags: -I${includedir}/lz4-@LZ4_API_VERSION@ -I${libdir}/lz4-@LZ4_API_VERSION@/include
